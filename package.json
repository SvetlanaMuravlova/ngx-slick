{
  "name": "ngx-slick",
  "version": "0.2.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:production": "ng run ngx-slick:build:production",
    "build:watch": "npm run build -- --watch",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-slick -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devmark/ngx-slick"
  },
  "author": {
    "name": "Mark",
    "email": "hc.devmark@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devmark/ngx-slick/issues"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.3.13",
    "@types/node": "^15.12.2",
    "angular-in-memory-web-api": "^0.11.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.0",
    "core-js": "^3.14.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-rollup": "^2.17.0",
    "gulp4-run-sequence": "^1.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.6.1",
    "ng-packagr": "^11.2.0",
    "node-sass": "^6.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.7.1",
    "protractor": "^7.0.0",
    "rollup": "^2.52.2",
    "rxjs": "6.5.5",
    "systemjs": "^6.10.1",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.1.5",
    "zone.js": "~0.11.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {
    "@types/slick-carousel": "^1.6.32",
    "slick-carousel": "^1.8.1"
  }
}
